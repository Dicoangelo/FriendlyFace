# FriendlyFace Environment Configuration
# Copy to .env and fill in values for your deployment.

# Storage backend: "sqlite" (default) or "supabase"
FF_STORAGE=sqlite

# Supabase (required when FF_STORAGE=supabase)
SUPABASE_URL=
SUPABASE_KEY=

# API key auth (comma-separated; leave empty to disable auth in dev mode)
FF_API_KEYS=

# Server
FF_HOST=0.0.0.0
FF_PORT=8000
